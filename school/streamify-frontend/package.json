		"typescript": "5.1.6",
		"yup": "1.2.0"
	},
	"devDependencies": {
		"@testing-library/jest-dom": "^6.0.1",
		"@testing-library/react": "14.0.0",
		"@types/event-source-polyfill": "1.0.1",
		"@types/jest": "29.5.4",
		"@types/node": "20.5.3",
		"@types/react": "18.2.21",
